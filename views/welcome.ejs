      .icon-check img {
        width: 15rem;
      }

      .curve {
        width: 100%;
        transform: translateY(1rem);
      }

      button {
        display: flex;
        flex-wrap: nowrap;
        align-items: center;
        justify-content: center;
        border: none;
        background-color: #582456;
        font-size: 1rem;
        padding: 0.5rem 0.75rem;
        cursor: pointer;
        border-radius: 5rem;
        margin: 2rem;
        transition: 500ms;
        box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.2);
      }

      button div {
        margin: 0 0.5rem;
      }

      button div:first-child {
        margin-left: 1rem;
      }

      button div:last-child {
        margin-right: 1rem;
        transition: 250ms;
      }

      button:hover div:last-child {
        transform: rotate(45deg);
      }
